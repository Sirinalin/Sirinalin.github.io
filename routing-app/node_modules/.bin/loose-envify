#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/asus/codes/profile/Sirinalin.github.io/routing-app/node_modules/loose-envify/node_modules:/mnt/c/Users/asus/codes/profile/Sirinalin.github.io/routing-app/node_modules:/mnt/c/Users/asus/codes/profile/Sirinalin.github.io/node_modules:/mnt/c/Users/asus/codes/profile/node_modules:/mnt/c/Users/asus/codes/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/codes/profile/Sirinalin.github.io/routing-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/asus/codes/profile/Sirinalin.github.io/routing-app/node_modules/loose-envify/node_modules:/mnt/c/Users/asus/codes/profile/Sirinalin.github.io/routing-app/node_modules:/mnt/c/Users/asus/codes/profile/Sirinalin.github.io/node_modules:/mnt/c/Users/asus/codes/profile/node_modules:/mnt/c/Users/asus/codes/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/codes/profile/Sirinalin.github.io/routing-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../loose-envify/cli.js" "$@"
fi
